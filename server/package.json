{
  "name":"@rpx_/mcpx",
  "version": "1.0.1",
  "description": "A reusable and CLI-accessible Model Context Protocol (MCP) server for mcpX.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "mcpX": "./dist/index.js"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsc && node -e \"const fs=require('fs'); const content=fs.readFileSync('dist/index.js','utf8'); fs.writeFileSync('dist/index.js','#!/usr/bin/env node\\n'+content);\"",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "server",
    "mcpX",
    "extension",
    "browsermcp",
    "browser"
  ],
  "author": "Rudra Pratap Singh",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "zod": "^3.25.76"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rudra78996/mcpX"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "typescript": "^5.8.3"
  }
}
